@model IEnumerable<string>

@{
    ViewData["Title"] = "Тестирование Web API";
}


<div class="container">
    <h1>@ViewData["Title"]</h1>
    <table class="table">
        <thead>
            <tr>
                <th>
                    id
                </th>
                <th>
                    value
                </th>
            </tr>
        </thead>
        <tbody>
            @{ int i = 0;}
            @foreach (var item in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>@item</td>
                </tr>
            }
        </tbody>
    </table>
</div>
